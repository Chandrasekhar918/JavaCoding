package programs;

public class reversenum {

	public static void main(String[] args) {
		int n=1234;
		int r,s;
		for(int i=0;i<=n;i++) {
			if(n==1) {
				System.out.println(1);
			}
			r=n%10; // 4 3 2
			System.out.print(r);
			n=n/10;// 123 12 1
			
		}
	
		// TODO Auto-generated method stub

	}

}
