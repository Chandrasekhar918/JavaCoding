package programs;

public class largestamong3 {

	public static void main(String[] args) {
	int arr[]= {1,33,222};
	for(int i=0;i<arr.length;i++) {
		int a=arr[0];  //1
		if(a< arr[i]) {// 1< 33  ,1<222
			arr[0]=arr[i];//  33
			
			
		}
		System.out.println(arr[0]);
		
	}
	
		
		// TODO Auto-generated method stub

	}

}
